{
  "name": "AmplifyRTNCore",
  "version": "1.1.6",
  "summary": "React Native core module for aws-amplify",
  "homepage": "https://docs.amplify.aws/",
  "license": "Apache-2.0",
  "authors": "Amazon Web Services",
  "platforms": {
    "ios": "12.4"
  },
  "source": {
    "git": "https://github.com/aws-amplify/amplify-js.git",
    "tag": "1.1.6"
  },
  "source_files": "ios/**/*.{h,m,mm,swift,c}",
  "dependencies": {
    "React-Core": [],
    "React-Codegen": [],
    "RCT-Folly": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/core": []
  },
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32 -DRCT_NEW_ARCH_ENABLED=1",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\"",
    "OTHER_CPLUSPLUSFLAGS": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17"
  }
}
